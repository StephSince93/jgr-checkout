{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    <strong>{{name}}</strong>\n                    {{else}}\n                    {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/header":"<!-- JTB4 200320: TEMPORARY COVID-19 BANNER AT TOP OF ALL PAGES **PAUSED** --\n<div class=\"banners\" data-banner-location=\"top\">\n    <div class=\"banner\">\n        <span style=\"font-size:1.25rem; color:#fff;\">We are <a href=\"/covid-19-update/\" class=\"link-no-underline\">still open for business!</a> &nbsp;Read our <a href=\"/covid-19-update/\" class=\"link-no-underline\"><b>Covid-19 Update</b></a> here...\n        </span>\n    </div>\n</div>\n<!-- END PAUSE -->\n\n{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#each (limit banners.top_metadata 1)}}\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n        <div data-event-type=\"promotion-click\">\n            {{{this.content}}}\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{/if}}\n\n<header class=\"header\" role=\"banner\">\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n    </a>\n    {{> components/common/navigation}}\n\n    {{#if page_type '===' 'default'}}\n        <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </h1>\n    {{else}}\n        <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n            {{> components/common/store-logo}}\n        </div>\n    {{/if}}\n    {{{region name=\"header_bottom\"}}}\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\n        {{> components/common/navigation-menu}}\n    </div>\n</header>\n","components/common/navigation":"<nav class=\"navUser\">\n    {{> components/common/currency-selector}}\n<!-- Begin move Social in Navigation -- JTB4 190116 -->\n        {{#if theme_settings.social_icon_placement_top}}\n            <span class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </span>\n        {{/if}}\n<!-- End move Social in Navigation -- JTB4 190116 -->\n\n<!-- Display company phone number -- JTB4 191111 -->\n    <!-- span class=\"navUser-item navUser-action\" style=\"margin:0; font-size:1.0rem !important; font-weight:700 !important;\">{{settings.phone_number}}</span -->\n    <a href=\"tel:1{{settings.phone_number}}\" class=\"navUser-item navUser-action\" style=\"margin:0; font-size:1.0rem !important; font-weight:700 !important;\">{{settings.phone_number}}</a>\n\n<!-- Display Made in America -- JTB4 191118 -->\n    <a href=\"/about/\" class=\"navUser-item navUser-action\" style=\"margin-left:-5px; font-size:1.0rem !important\"><img src=\"/content/us_flag.png\" style=\"width: 29px; margin-right:3px;\">\n        <span class=\"min930\"> Made in America</span>\n    </a>    \n<!-- END PAUSE -->\n\n