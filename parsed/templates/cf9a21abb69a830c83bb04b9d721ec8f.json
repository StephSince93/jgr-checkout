{"components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                \n<!-- Hide default logo (too fuzzy!) and insert uncompressed, animated logo gif on desktop JTB4 190430 --\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}?imbypass=on\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n-->\n                <img class=\"header-logo-image lazyloaded\" src=\"/content/jgr_animated_logo.gif?imbypass=on&{{moment example format=\"mm\"}}\" width=\"100px\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\" />\n\n<!-- Add mobile logo. Manipulate hide and display logos via SCSS JTB4 191021 -->\n                <img class=\"header-logo-image-mobile\" src=\"/content/jack_only.png?imbypass=on\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\" />\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"}
